{
  "name": "cyclebuddy-stellar",
  "version": "1.0.0",
  "description": "A Web3-powered menstrual health companion built on Stellar",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "build:contracts": "bash scripts/build-contracts.sh",
    "deploy:contracts:testnet": "bash scripts/deploy-contracts.sh",
    "deploy:testnet": "stellar-cli deploy --network testnet",
    "deploy:mainnet": "stellar-cli deploy --network mainnet"
  },
  "dependencies": {
    "stellar-sdk": "^11.0.0",
    "soroban-client": "^1.0.0",
    "@stellar/freighter-api": "^1.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "@tanstack/react-query": "^4.32.6",
    "@reduxjs/toolkit": "^1.9.5",
    "react-redux": "^8.1.2",
    "axios": "^1.4.0",
    "ipfs-http-client": "^60.0.0",
    "ethers": "^6.7.0",
    "date-fns": "^2.30.0",
    "zod": "^3.21.4",
    "tailwindcss": "^3.3.3",
    "@headlessui/react": "^1.7.16",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.15.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.2",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.28"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/cyclebuddy-stellar.git"
  },
  "keywords": [
    "stellar",
    "web3",
    "blockchain",
    "health",
    "privacy",
    "education"
  ],
  "author": "CycleBuddy Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/cyclebuddy-stellar/issues"
  },
  "homepage": "https://github.com/yourusername/cyclebuddy-stellar#readme"
}
 